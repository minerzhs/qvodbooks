<%= form_for @record do |f| %>
  <p>
    <%= f.label 'Book ID' %> <br>
    <%= f.text_field :book_id %>
  </p>

  <p>
    <%= f.label 'User' %> <br>
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.label 'Check Out Date' %> <br>
    <%= f.text_field :check_out_date %>
  </p>  

  <p>
    <%= f.submit %>
  </p>

<% end %>